{
    "name": "sebdesign/laravel-sri",
    "description": "Subresource Integrity (SRI) package for Laravel",
    "keywords": [
        "laravel",
        "sri"
    ],
    "homepage": "https://github.com/sebdesign/laravel-sri",
    "type": "library",
    "require": {
        "php": "^7.1",
        "laravel/framework": "^5.5|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.5"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Sebdesign\\SRI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sebdesign\\SRI\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "SÃ©bastien Nikolaou",
            "email": "info@sebdesign.eu",
            "homepage": "http://sebdesign.eu"
        }
    ],
    "extra": {
        "laravel": {
            "providers": ["Sebdesign\\SRI\\SubresourceIntegrityServiceProvider"]
        }
    }
}
